                              "itemSpacing": 8,
                              "children": [
                                {
                                  "id": "legend-color-expired",
                                  "name": "Legend Color - Expired",
                                  "type": "RECTANGLE",
                                  "fills": [{ "type": "SOLID", "color": "#DC2626" }],
                                  "cornerRadius": 4
