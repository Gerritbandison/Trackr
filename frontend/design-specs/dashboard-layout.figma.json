                                {
                                  "id": "legend-label-expired",
                                  "name": "Legend Label - Expired",
                                  "type": "TEXT",
                                  "characters": "Expired",
                                  "style": {
                                    "fontSize": 12,
                                    "fontWeight": 400,
                                    "color": "#64748B"
                                  }
                                }
                              ]
                            },
                            {
                              "id": "legend-item-expiring-soon",
                              "name": "Legend - Expiring Soon",
                              "type": "FRAME",
                              "layoutMode": "HORIZONTAL",
                              "itemSpacing": 8,
                              "children": [
                                {
                                  "id": "legend-color-expiring-soon",
