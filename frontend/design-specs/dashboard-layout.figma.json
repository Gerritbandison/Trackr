                          "layoutMode": "GRID",
                          "padding": { "top": 16, "right": 16, "bottom": 16, "left": 16 },
                          "itemSpacing": 8,
                          "gridColumns": 12,
                          "constraints": {
                            "horizontal": "STRETCH",
                            "vertical": "FILL"
                          },
                          "children": [
                            {
                              "id": "heatmap-cell-placeholder",
                              "name": "Heatmap Cell Placeholder",
                              "type": "FRAME",
                              "constraints": {
                                "horizontal": "FILL",
                                "vertical": "FILL"
