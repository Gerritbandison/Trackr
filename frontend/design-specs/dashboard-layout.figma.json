                      "flex": 1,
                      "fills": [{ "type": "SOLID", "color": "#FFFFFF" }],
                      "cornerRadius": 16,
                      "effects": [
                        {
                          "type": "DROP_SHADOW",
                          "color": "#00000008",
                          "offset": { "x": 0, "y": 4 },
                          "radius": 12,
                          "spread": 0
                        }
                      ],
                      "children": [
                        {
                          "id": "compliance-gauge-header",
                          "name": "Compliance Gauge Header",
                          "type": "FRAME",
                          "layoutMode": "HORIZONTAL",
                          "padding": { "top": 0, "right": 0, "bottom": 16, "left": 0 },
