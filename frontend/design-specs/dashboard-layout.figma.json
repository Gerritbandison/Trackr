                    "sm": {
                      "layoutMode": "VERTICAL",
                      "itemSpacing": 16
                    },
                    "md": {
                      "layoutMode": "HORIZONTAL",
                      "itemSpacing": 24
                    },
                    "lg": {
                      "layoutMode": "HORIZONTAL",
                      "itemSpacing": 24
                    }
                  }
