                              "children": []
                            },
                            {
                              "id": "asset-card-placeholder-2",
                              "name": "Asset Card 2",
                              "type": "FRAME",
                              "layoutMode": "HORIZONTAL",
                              "padding": { "top": 16, "right": 16, "bottom": 16, "left": 16 },
                              "itemSpacing": 12,
                              "constraints": {
                                "horizontal": "STRETCH",
                                "vertical": "TOP"
                              },
                              "fills": [{ "type": "SOLID", "color": "#F8FAFC" }],
                              "cornerRadius": 12,
                              "children": []
