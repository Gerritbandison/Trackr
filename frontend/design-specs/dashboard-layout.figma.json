                                  "type": "RECTANGLE",
                                  "fills": [{ "type": "SOLID", "color": "#F59E0B" }],
                                  "cornerRadius": 4
                                },
                                {
                                  "id": "legend-label-expiring-soon",
                                  "name": "Legend Label - Expiring Soon",
                                  "type": "TEXT",
                                  "characters": "Expiring Soon",
                                  "style": {
                                    "fontSize": 12,
                                    "fontWeight": 400,
                                    "color": "#64748B"
                                  }
                                }
                              ]
                            },
                            {
                              "id": "legend-item-valid",
                              "name": "Legend - Valid",
                              "type": "FRAME",
                              "layoutMode": "HORIZONTAL",
                              "itemSpacing": 8,
                              "children": [
                                {
                                  "id": "legend-color-valid",
                                  "name": "Legend Color - Valid",
                                  "type": "RECTANGLE",
                                  "fills": [{ "type": "SOLID", "color": "#10B981" }],
                                  "cornerRadius": 4
                                },
                                {
                                  "id": "legend-label-valid",
                                  "name": "Legend Label - Valid",
                                  "type": "TEXT",
                                  "characters": "Valid",
                                  "style": {
                                    "fontSize": 12,
                                    "fontWeight": 400,
                                    "color": "#64748B"
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ],
              "responsive": {
                "sm": {
                  "itemSpacing": 16
                },
                "md": {
                  "itemSpacing": 20
                },
                "lg": {
                  "itemSpacing": 24
                }
              }
            }
          ]
        }
