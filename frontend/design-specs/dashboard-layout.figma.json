                            }
                          ]
                        },
                        {
                          "id": "asset-list-content",
                          "name": "Asset List Content",
                          "type": "FRAME",
                          "layoutMode": "VERTICAL",
                          "itemSpacing": 12,
                          "constraints": {
                            "horizontal": "STRETCH",
                            "vertical": "FILL"
                          },
                          "children": [
                            {
                              "id": "asset-card-placeholder-1",
                              "name": "Asset Card 1",
                              "type": "FRAME",
                              "layoutMode": "HORIZONTAL",
