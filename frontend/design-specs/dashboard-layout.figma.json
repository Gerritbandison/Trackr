                            {
                              "id": "compliance-gauge-title",
                              "name": "Compliance Gauge Title",
                              "type": "TEXT",
                              "characters": "Compliance Status",
                              "style": {
                                "fontSize": 20,
                                "fontWeight": 600,
                                "color": "#0F172A"
                              }
                            }
                          ]
                        },
                        {
                          "id": "compliance-gauge-content",
                          "name": "Compliance Gauge Content",
                          "type": "FRAME",
                          "layoutMode": "VERTICAL",
                          "itemSpacing": 16,
                          "constraints": {
                            "horizontal": "STRETCH",
                            "vertical": "FILL"
                          },
                          "children": [
                            {
                              "id": "gauge-circle",
                              "name": "Gauge Circle Placeholder",
                              "type": "ELLIPSE",
                              "constraints": {
                                "horizontal": "CENTER",
                                "vertical": "CENTER"
