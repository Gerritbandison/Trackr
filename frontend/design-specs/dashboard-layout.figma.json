{
  "name": "Trackr ITAM Dashboard Layout",
  "type": "DOCUMENT",
  "version": "1.0.0",
  "children": [
    {
      "id": "dashboard-page",
      "name": "Main Dashboard",
      "type": "CANVAS",
      "backgroundColor": "#F8FAFC",
      "children": [
        {
          "id": "dashboard-container",
          "name": "Dashboard Container",
          "type": "FRAME",
          "layoutMode": "VERTICAL",
          "padding": { "top": 24, "right": 24, "bottom": 24, "left": 24 },
          "itemSpacing": 24,
          "constraints": {
            "horizontal": "STRETCH",
            "vertical": "STRETCH"
          },
          "children": [
            {
              "id": "dashboard-header",
              "name": "Header Section",
              "type": "FRAME",
              "layoutMode": "HORIZONTAL",
              "padding": { "top": 16, "right": 24, "bottom": 16, "left": 24 },
              "itemSpacing": 16,
              "constraints": {
                "horizontal": "STRETCH",
                "vertical": "TOP"
              },
              "children": [
                {
                  "id": "header-title",
                  "name": "Dashboard Title",
                  "type": "TEXT",
                  "characters": "IT Asset Management Dashboard",
                  "style": {
                    "fontSize": 32,
                    "fontWeight": 700,
                    "color": "#0F172A"
                  }
                }
              ]
            },
            {
              "id": "main-grid",
              "name": "Main Grid Layout",
              "type": "FRAME",
              "layoutMode": "VERTICAL",
              "itemSpacing": 24,
              "constraints": {
                "horizontal": "STRETCH",
                "vertical": "STRETCH"
              },
              "children": [
                {
                  "id": "top-row",
                  "name": "Top Row - Asset List & Compliance Gauge",
                  "type": "FRAME",
                  "layoutMode": "HORIZONTAL",
                  "itemSpacing": 24,
                  "constraints": {
                    "horizontal": "STRETCH",
                    "vertical": "TOP"
                  },
                  "children": [
                    {
                      "id": "asset-list-container",
                      "name": "Asset List Container",
                      "type": "FRAME",
                      "layoutMode": "VERTICAL",
                      "padding": { "top": 24, "right": 24, "bottom": 24, "left": 24 },
                      "constraints": {
                        "horizontal": "FILL",
                        "vertical": "STRETCH"
                      },
                      "flex": 2,
                      "fills": [{ "type": "SOLID", "color": "#FFFFFF" }],
                      "cornerRadius": 16,
                      "effects": [
                        {
                          "type": "DROP_SHADOW",
                          "color": "#00000008",
                          "offset": { "x": 0, "y": 4 },
                          "radius": 12,
                          "spread": 0
                        }
                      ],
                      "children": [
                        {
                          "id": "asset-list-header",
                          "name": "Asset List Header",
                          "type": "FRAME",
                          "layoutMode": "HORIZONTAL",
                          "padding": { "top": 0, "right": 0, "bottom": 16, "left": 0 },
                          "itemSpacing": 12,
                          "children": [
                            {
                              "id": "asset-list-title",
                              "name": "Asset List Title",
                              "type": "TEXT",
                              "characters": "Asset Inventory",
                              "style": {
                                "fontSize": 20,
                                "fontWeight": 600,
                                "color": "#0F172A"
