                    {
                      "id": "compliance-gauge-container",
                      "name": "Compliance Gauge Container",
                      "type": "FRAME",
                      "layoutMode": "VERTICAL",
                      "padding": { "top": 24, "right": 24, "bottom": 24, "left": 24 },
                      "constraints": {
                        "horizontal": "FILL",
                        "vertical": "STRETCH"
