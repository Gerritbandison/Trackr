                          "children": [
                            {
                              "id": "heatmap-title",
                              "name": "Heatmap Title",
                              "type": "TEXT",
                              "characters": "License Expiry Heatmap",
                              "style": {
                                "fontSize": 20,
                                "fontWeight": 600,
